                    "enableOpenblas": true,
                    "enableScalapack": true,
                    "enableZlib": true,
                    "enableBzip2": true,
                    "enableBoost": true,
                    "enableGit": true,
                    "enableCmake": true,
                    "enableNumdiff": true,
                    "enableAstyle": true,
                    "enableMkl": false,
