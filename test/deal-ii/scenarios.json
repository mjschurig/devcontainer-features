{
  "default": {
    "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
    "features": {
      "deal-ii": {}
    }
  },
  "default_debian": {
    "image": "mcr.microsoft.com/devcontainers/base:debian",
    "features": {
      "deal-ii": {}
    }
  },
  "with_mpi": {
    "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
    "features": {
      "deal-ii": {
        "enableMPI": true,
        "buildThreads": "2"
      }
    }
  },
  "with_petsc": {
    "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
    "features": {
      "deal-ii": {
        "enablePETSc": true,
        "buildThreads": "2"
      }
    }
  },
  "with_trilinos": {
    "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
    "features": {
      "trilinos": {},
      "deal-ii": {
        "enableTrilinos": true,
        "buildThreads": "2"
      }
    }
  },
  "full_features": {
    "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
    "features": {
      "trilinos": {},
      "deal-ii": {
        "enableMPI": true,
        "enablePETSc": true,
        "enableTrilinos": true,
        "buildThreads": "2"
      }
    }
  },
  "older_version": {
    "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
    "features": {
      "deal-ii": {
        "version": "9.5.0",
        "buildThreads": "2"
      }
    }
  },
  "single_thread": {
    "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
    "features": {
      "deal-ii": {
        "buildThreads": "1"
      }
    }
  },
  "idempotency": {
    "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
    "features": {
      "deal-ii": {}
    },
    "runTwice": true
  }
